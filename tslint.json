{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "indent": [
            true,
            "spaces",
            2
        ],
        "no-console": false,
        "curly": true,
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],
        "member-access": [
            true,
            "no-public"
        ],
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "property-declaration"
        ],
        "no-var-requires": true,
        "prefer-const": true
    },
    "rulesDirectory": []
}